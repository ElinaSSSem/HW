using System;

 public class Program
 {
 	public static void Main()
 	{   
		int y = 0;
 		string d = Console.ReadLine();
         string[] n = d.Split(new char[] { ' ' });
         for (int i = 0; i<n.Length; i++)  
         { 
		int z = int.Parse(n[i]);	 
 	  if (z > 0) y++; 
         }
		Console.WriteLine(y);
 	}
 }
