/*
 Напишите программу, которая считывает значения двух целочисленных переменных a и b, 
затем меняет их значения местами (то есть в переменной a должно быть записано то, что раньше хранилось 
в b, а в переменной b записано то, что раньше хранилось в a). Затем выведите значения переменных.

Входные данные
Вводятся целые числа a и b.

Выходные данные
Выведите ответ на задачу.
 */


using System;

namespace ConsoleApp11
{
    class Program
    {
        static void Main(string[] args)
        {
            int a = Convert.ToInt32(Console.ReadLine());
            int b = Convert.ToInt32(Console.ReadLine());
            int c = a;
            a = b;
            b = c;
            Console.Write($"теперь это {a} и {b}");

        }
    }
}
