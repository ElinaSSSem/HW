/*
Дано натуральное число. Выведите его последнюю цифру.

Входные данные
Вводится натуральное число.

Выходные данные
Выведите ответ на задачу.
*/


using System;

namespace ConsoleApp8
{
    class Program
    {
        static void Main(string[] args)
        {
            string s = Convert.ToString(Console.ReadLine());
            int i = s.Length;
            char t = s[i-1];
            Console.WriteLine(t);
        }
    }
}
